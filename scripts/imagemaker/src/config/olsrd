config 'olsrd'
	option 'IpVersion' '4'
	option 'FIBMetric' 'flat'
	option 'LinkQualityAlgorithm' 'etx_ff'
  option 'DebugLevel' '0'

config 'Interface'
	option 'ignore' '0'
	option 'interface' 'vpn'

config 'Interface'
        option 'ignore' '1'
        option 'interface' 'wan'

config 'LoadPlugin'
	option 'library' 'olsrd_dyn_gw_plain.so.0.4'
  option 'ignore' '0'

config 'LoadPlugin'
	option 'library' 'olsrd_txtinfo.so.0.1'
	option 'accept' '127.0.0.1'

config 'Interface'
  option 'ignore' '0'
  option 'interface' 'wifi0'

#config 'Interface'
#  option 'ignore' '0'
#  option 'interface' 'lan'

config 'LoadPlugin'
  option 'interval' '30'
  option 'file' '/var/run/olsrd.watchdog'
  option 'library' 'olsrd_watchdog.so.0.1'

config 'LoadPlugin'
  option 'library' 'olsrd_nameservice.so.0.3'
  option 'suffix' '.olsr'
  option 'hosts_file' '/var/etc/hosts.olsr'

